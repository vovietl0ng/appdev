@model List<appdev.Models.ApplicationUser>
@{
    ViewBag.Title = "UserView";
}

<h2>User View</h2>

@Html.ActionLink("Back", "Index", "Admin", null, new { @class = "btn btn-primary" })
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">User Name</th>
            <th scope="col">Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.UserName</td>
                <td>@item.Email</td>
            </tr>
        }
    </tbody>
</table>



